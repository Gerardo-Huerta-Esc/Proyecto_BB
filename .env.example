# Grafana
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Postgres Exporter
POSTGRES_EXPORTER_PORT=9187

# ETL Worker
STAGE_TARGET_TXN=1000000
